{% extends 'base.twig' %}
{% block content %}
    {% include 'components/hero.twig' with {
        'heading': hero.heading,
        'description': hero.description,
        'button_label': hero.button_label,
        'images': hero.images,
    } %}
    {% include 'components/why-us.twig' with {
        'heading': why_us.heading,
        'description': why_us.description,
        'cards': why_us.cards,
    } %}
    {% include 'components/testimonials.twig' with {
        'heading': testimonials.heading,
        'total_score': testimonials.total_score,
        'testimonials': testimonials.testimonials,
        'background_image': testimonials.background_image,
    } %}
    {% include 'components/how-it-works.twig' with {
        'heading': how_it_works.heading,
        'steps': how_it_works.steps,
        'footnote': how_it_works.footnote,
        'form_title': how_it_works.form_title,
    } %}
    {% include 'components/services.twig' with {
        'heading': services.heading,
        'description': services.description,
        'services_list_title': services.services_list_title,
        'services': services.services,
    } %}
    {% include 'components/cta.twig' with {
        'heading': cta.heading,
        'description': cta.description,
        'background_image': cta.background_image,
        'first_cta': cta.first_cta_link,
        'second_cta': cta.second_cta_link,
    } %}
    {% include 'components/projects.twig' with {
        'heading': projects.heading,
        'gallery': projects.gallery,
    } %}
    {% include 'components/about-us.twig' with {
        'sections': about_us.sections,
    } %}
    {% include 'components/contact-us.twig' with {
        'heading': contact_us.heading,
        'description': contact_us.description,
        'button_label': contact_us.button_label,
        'background_image': contact_us.background_image,
    } %}
{% endblock %}
