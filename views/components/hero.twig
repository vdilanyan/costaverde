<div id="hero">
    <div class="container">
        <div class="content-wrapper">
            <h1 class="font-bold section-title line-height-small">{{ heading }}</h1>
            <p>{{ description }}</p>
            <a href="#how-it-works" class="btn">{{ button_label }}</a>
        </div>
    </div>
    <div class="container images-wrapper">
        <div class="images flex-container">
            {% for item in images %}
                <img src="{{ item.image.url }}" alt="">
            {% endfor %}
        </div>
    </div>
</div>
